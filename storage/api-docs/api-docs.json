                                    "properties": {
                                        "erro": {
                                            "description": "Mensagem de erro indicando que as credenciais são inválidas",
                                            "type": "string",
                                            "example": "Usuário ou senha inválido!"
                                        }
                                    },
                                    "type": "object"
                                }
                            }
                        }
                    }
                }
            }
        },
